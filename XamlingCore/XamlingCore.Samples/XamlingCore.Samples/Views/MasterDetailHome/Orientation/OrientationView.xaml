<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
					   xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
					   x:Class="XamlingCore.Samples.Views.MasterDetailHome.Orientation.OrientationView">

  <StackLayout>
    <Button Command="{Binding PortraitCommand}" Text="Portrait"/>
    <Button Command="{Binding LandscapeCommand}" Text="Landscape"/>
    <Button Command="{Binding BothCommand}" Text="Both"/>

    <Button Command="{Binding PopCommand}" Text="Pop"/>
    <Button Command="{Binding PushCommand}" Text="Push"/>
  </StackLayout>
</ContentPage>